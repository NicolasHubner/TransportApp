{
    "env": {
        "es2021": true,
        "node":true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "__DEV__": "readonly"
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "react-native"],
    "rules": {
        "no-console": "off",
        "react/prop-types": "off"
    }
}
